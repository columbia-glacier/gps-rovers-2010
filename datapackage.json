{
  "name": "gps-rovers-2010",
  "title": "GPS Rover Positions (2010)",
  "description": "Daily to 30-second positions of six GPS rovers placed on the glacier surface in 2010.",
  "version": "0.1.0",
  "contributors": [
    {
      "title": "Ethan Welty",
      "email": "ethan.welty@gmail.com",
      "role": "author"
    },
    {
      "title": "Ian Howat",
      "role": "Coordinated the research."
    },
    {
      "title": "Shad O'Neel",
      "role": "Assisted the research."
    },
    {
      "title": "Alberto Behar",
      "role": "Designed and built the GPS rovers."
    },
    {
      "title": "Julie Markus",
      "role": "Processed the NETRS positions."
    },
    {
      "title": "Phaedra Tinder",
      "role": "Processed the EDGEE positions."
    }
  ],
  "sources": [
    {
      "title": "Original data, scripts, and documentation",
      "path": "sources/"
    }
  ],
  "resources": [
    {
      "name": "netrs",
      "title": "Upper Glacier Rover Positions (NETRS)",
      "description": "Daily positions for the three rovers above Divider Mountain, processed using PPP from NETRS data. Positions for each rover are split into two tracks due to the dig-out on 2010-05-17 (Ian Howat, `sources/Columbia netrs daily solutions.eml`).",
      "path": "data/netrs.csv",
      "schema": {
        "fields": [
          {
            "name": "id",
            "type": "string",
            "description": "Grouping variable (rover_id.track#)."
          },
          {
            "name": "t",
            "type": "date",
            "description": "Date (time and time zone unknown).",
            "format": "%Y-%m-%d"
          },
          {
            "name": "x",
            "type": "number",
            "description": "Easting in WGS84 UTM Zone 6N (EPSG:32606).",
            "unit": "m"
          },
          {
            "name": "y",
            "type": "number",
            "description": "Northing (m) in WGS84 UTM Zone 6N (EPSG:32606).",
            "unit": "m"
          },
          {
            "name": "z",
            "type": "number",
            "description": "Height above the WGS84 ellipsoid.",
            "unit": "m"
          }
        ]
      }
    },
    {
      "name": "edgee",
      "title": "Lower Glacier Rover Positions (EDGEE)",
      "description": "30-second positions for the three rovers below Divider Mountain, processed from EDGEE data using GrafNav. Positions for some rovers are split into multiple tracks due to relocations (Ian Howat, `sources/columbia_edgee_2010.m`).",
      "path": "data/edgee.csv",
      "schema": {
        "fields": [
          {
            "name": "id",
            "type": "string",
            "description": "Grouping variable (rover_id.track#)."
          },
          {
            "name": "t",
            "type": "datetime",
            "description": "Date and time (UTC).",
            "format": "%Y-%m-%dT%H:%M:%SZ"
          },
          {
            "name": "x",
            "type": "number",
            "description": "Easting in WGS84 UTM Zone 6N (EPSG:32606).",
            "unit": "m"
          },
          {
            "name": "y",
            "type": "number",
            "description": "Northing in WGS84 UTM Zone 6N (EPSG:32606).",
            "unit": "m"
          },
          {
            "name": "z",
            "type": "number",
            "description": "Height above the WGS84 ellipsoid.",
            "unit": "m"
          },
          {
            "name": "xy_sd",
            "type": "number",
            "description": "Standard deviation in horizontal position (x, y).",
            "unit": "m"
          },
          {
            "name": "z_sd",
            "type": "number",
            "description": "Standard deviation in vertical position (z).",
            "unit": "m"
          },
          {
            "name": "quality",
            "type": "integer",
            "description": "GrafNav position quality metric (smaller number = higher quality)."
          }
        ]
      }
    }
  ]
}
